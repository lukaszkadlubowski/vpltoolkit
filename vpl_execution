#!/bin/bash

rm -f $0 # for security issue

### load environment
[ -z "$RUNDIR" ] && RUNDIR=$HOME
source $RUNDIR/vplmodel/vplmodel.sh
vplmodel_loadenv
vplmodel_checkenv
vplmodel_exportenv

### run exo
BASEDIR="$RUNDIR/GIT/$EXO"
[ ! -f $BASEDIR/run.sh ] && echo "âš  File \"run.sh\" missing!" && exit 0
cd $RUNDIR
./GIT/$EXO/run.sh

# EOF
